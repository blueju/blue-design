{
  "private": true,
  "name": "blued",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.16.10",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.5.15",
    "dumi": "^1.1.24",
    "father-build": "^1.19.7",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "yorkie": "^2.0.0"
  }
}
